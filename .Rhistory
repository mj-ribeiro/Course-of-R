neural = function(apr){
e_t = 1
while(e_t !=0){
e_t = 0
for (i in 1:length(s)){
s_cal = step(ent[i, ]%*%pes)
e = abs(s[i] - s_cal)
e_t = e_t + e
for (j in 1:length(pes)) {
pes[j] = pes[j] + apr * ent[i, j] * e
}
}
}
cat('Os pesos são:', pes)
}
